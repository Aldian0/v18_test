<?xml version='1.0' encoding='utf-8'?>
<odoo>
        <data>
            <!-- Add you code here -->
        <record id="seq_shipment_fragile" model="ir.sequence">
            <field name="name">Nomor Pengiriman Fragile</field>
            <field name="code">shipment.fragile</field>
            <field name="prefix">NP/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_shipment_priority" model="ir.sequence">
            <field name="name">Nomor Pengiriman Priority</field>
            <field name="code">shipment.priority</field>
            <field name="prefix">SP/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        </data>
</odoo>